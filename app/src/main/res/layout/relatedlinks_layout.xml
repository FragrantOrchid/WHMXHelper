<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <Button
        android:id="@+id/official_website"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="官网链接"/>
    <Button
        android:id="@+id/wiki_website"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="物华弥新wiki"/>
    <Button
        android:id="@+id/github_website"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="源代码链接"/>
    <Button
        android:id="@+id/api_website"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="后端api"/>
    <Button
        android:id="@+id/ifdian_website"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="爱发电链接"/>
    <Button
        android:id="@+id/email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="错误反馈:257880872@qq.com"/>

</LinearLayout>